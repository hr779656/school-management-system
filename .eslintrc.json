{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "no-unused-vars": "error",
        "no-console": "on"
    }
}
