generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:j4asbc56@localhost:5432/SMSDb?schema=public"
}

model Users {
  id        Int      @id @default(autoincrement())
  name      String
  password  String
  role      Role     @default(USER)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

enum Role {
  USER
  ADMIN
} 




